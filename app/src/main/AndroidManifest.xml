<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gxu.gxuproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/bywf"
        android:label="@string/app_name"
        android:theme="@style/TimeTheme">
        <activity
            android:name=".home.SplashActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Black.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".home.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/TimeTheme"></activity>
        <activity
            android:name=".home.SingleActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".home.MultipleActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".home.MainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
